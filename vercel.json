{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "installCommand": "yarn install",
  "public": false,
  "outputDirectory": "public",
  "rewrites": [
    {
      "source": "/",
      "destination": "/api"
    },
    {
      "source": "/(.+)",
      "destination": "/api/*"
    }
  ]
}
